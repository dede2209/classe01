class Empregado:
    def __init__(self, nome, salario):
        self.nome = nome
        self.salario = salario

    def getNome(self):
        return self.nome

    def getSalario(self):
        return self.salario

empregado1 = Empregado("Jo√£o", 2000.0)

print(empregado1.getNome())
print(empregado1.getSalario())
